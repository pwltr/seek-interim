        "browser-sync": "^2.21.0",
        "browser-sync-webpack-plugin": "^1.2.0",
        "cross-env": "^5.1",
        "gulp": "^3.9.1",
        "gulp-notify": "*",
        "gulp-svg-sprite": "^1.3.7",
        "gulp-watch": "^4.3.11",
        "laravel-mix": "^1.0",
        "lodash": "^4.17.4"
    },
    "dependencies": {
        "cash-dom": "^1.3.5",
        "classie": "^1.0.0",
        "imagesloaded": "^4.1.3",
        "normalize.css": "^7.0.0",
